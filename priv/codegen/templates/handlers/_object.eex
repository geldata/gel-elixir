%<%= @opts[:module] %>{
    <%= for property <- @object.properties do %>
        <%= if property.is_link_property do %>
    "@<%= property.name %>": <%= Gel.EdgeQL.Generator.Handler.handle(property, @opts) %>,
        <% else %>
    <%= property.name %>: <%= Gel.EdgeQL.Generator.Handler.handle(property, @opts) %>,
        <% end %>
    <% end %>

    <%= for link <- @object.links do %>
    <%= link.name %>: <%= Gel.EdgeQL.Generator.Handler.handle(link, @opts) %>,
    <% end %>
}
